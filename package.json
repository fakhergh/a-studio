{
    "name": "a-studio",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "prepare": "husky",
        "start": "serve -s dist",
        "dev": "vite --port 3000",
        "build": "tsc -b && vite build",
        "static:check": "eslint . && prettier --log-level silent --check --ignore-unknown .",
        "static:fix": "eslint . --fix && prettier --log-level silent --write --ignore-unknown .",
        "preview": "vite preview",
        "test": "vitest --config ./vitest.config.ts --run",
        "test:watch": "vitest --config ./vitest.config.ts"
    },
    "dependencies": {
        "@tanstack/react-router": "^1.109.2",
        "@tanstack/router-plugin": "^1.109.2",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "serve": "^14.2.4"
    },
    "devDependencies": {
        "@eslint/js": "^9.21.0",
        "@tanstack/router-devtools": "^1.109.2",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.2.0",
        "@types/node": "^22.13.5",
        "@types/react": "^19.0.8",
        "@types/react-dom": "^19.0.3",
        "@vitejs/plugin-react": "^4.3.4",
        "eslint": "^9.21.0",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.18",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "globals": "^15.15.0",
        "happy-dom": "^17.1.3",
        "husky": "^9.1.7",
        "prettier": "^3.5.2",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.24.1",
        "vite": "^6.1.0",
        "vitest": "^3.0.6"
    }
}
